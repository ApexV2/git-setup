{
  "name": "git-setup-cli",
  "version": "0.0.5",
  "description": "CLI tool to setup git repositories",
  "main": "dist/git-setup.js",
  "type": "module",
  "bin": {
    "git-setup": "./dist/git-setup.js"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "inquirer": "^8.2.6"
  },
  "pkg": {
    "targets": [
      "node16-win-x64"
    ],
    "outputPath": "dist"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && pkg . --targets node16-win-x64 --output dist/git-setup.exe",
    "start": "tsc && node .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "git",
    "cli",
    "setup",
    "repository"
  ],
  "author": "ApexV2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ApexV2/git-setup"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.8.6",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n": "^17.12.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unicorn": "^56.0.0",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.12.2"
  }
}
