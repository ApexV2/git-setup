{
  "name": "git-setup-cli",
  "version": "0.0.2",
  "description": "CLI tool to setup git repositories",
  "main": "bin/git-setup.js",
  "bin": {
    "git-setup": "./bin/git-setup.js"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.6"
  },
  "pkg": {
